<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<!-- This requires the presence of the 'openbox-menu' package to work -->
		<menu id="root-menu-88558" label="APPLICATIONS">
			<item label="Terminal">
				<action name="Execute">
					<execute>gnome-terminal</execute>
				</action>
			</item>
			<separator/>
			<item label="Text Edit">
				<action name="Execute">
					<execute>gedit --new-window %u</execute>
				</action>
			</item>
			<separator/>
			<item label="File Manager">
				<action name="Execute">
					<execute>thunar</execute>
				</action>
			</item>
			<item label="File Manager (root)">
				<action name="Execute">
					<execute>sudo thunar</execute>
				</action>
			</item>
			<item label="File Roller">
				<action name="Execute">
					<execute>file-roller</execute>
				</action>
			</item>
			<separator/>
			<item label="FireFox">
				<action name="Execute">
					<execute>/usr/lib/firefox-esr/firefox-esr</execute>
				</action>
			</item>
			<separator/>
			<item label="Gimp">
				<action name="Execute">
					<execute>gimp</execute>
				</action>
			</item>
			<item label="Media Player">
				<action name="Execute">
					<execute>/usr/bin/vlc</execute>
				</action>
			</item>
			<item label="Transmission">
				<action name="Execute">
					<execute>transmission-qt %U</execute>
				</action>
			</item>
			<separator/>
			<item label="VeraCrypt">
				<action name="Execute">
					<execute>/usr/bin/veracrypt</execute>
				</action>
			</item>
			<item label="BitLocker">
				<action name="Execute">
					<execute>/home/m/Stayboogy/apps/bitlocker-anywhere/bin/run.sh</execute>
				</action>
			</item>
			<item label="VMWare Player">
				<action name="Execute">
					<execute>/usr/bin/vmplayer</execute>
				</action>
			</item>
			<separator/>
			<item label="Meld">
				<action name="Execute">
					<execute>meld</execute>
				</action>
			</item>
			<item label="BleachBit (root)">
				<action name="Execute">
					<execute>sudo bleachbit</execute>
				</action>
			</item>
			<item label="Disk Usage">
				<action name="Execute">
					<execute>sudo baobab</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="root-menu-204132" label="CPU FREQ">
			<item label="Performance">
				<action name="Execute">
					<execute>sudo cpufreq-set -g performance</execute>
				</action>
			</item>
			<item label="Powersave">
				<action name="Execute">
					<execute>sudo cpufreq-set -g powersave</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="root-menu-892940" label="UTILITIES">
			<item label="Synaptic">
				<action name="Execute">
					<execute>sudo synaptic</execute>
				</action>
			</item>
			<item label="Deb Installer">
				<action name="Execute">
					<execute>sudo gdebi-gtk</execute>
				</action>
			</item>
			<item label="Disk Utility">
				<action name="Execute">
					<execute>sudo gnome-disks</execute>
				</action>
			</item>
			<item label="GParted">
				<action name="Execute">
					<execute>sudo /usr/sbin/gparted</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="root-menu-892057" label="SETTINGS">
			<item label="FileSettings">
				<action name="Execute">
					<execute>thunar-settings</execute>
				</action>
			</item>
			<item label="ScreenOn">
				<action name="Execute">
					<execute>/home/m/Stayboogy/cmds/noblacks</execute>
				</action>
			</item>
			<item label="Wallpaper">
				<action name="Execute">
					<execute>nitrogen</execute>
				</action>
			</item>
			<item label="Greeter">
				<action name="Execute">
					<execute>sudo lightdm-gtk-greeter-settings</execute>
				</action>
			</item>
			<item label="Theme">
				<action name="Execute">
					<execute>lxappearance</execute>
				</action>
			</item>
			<item label="Panel">
				<action name="Execute">
					<execute>tint2conf</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="root-menu-362342" label="BACKUP">
			<item label="Timeshift">
				<action name="Execute">
					<execute>sudo timeshift-gtk</execute>
				</action>
			</item>
			<item label="SystemBack">
				<action name="Execute">
					<execute>/usr/lib/systemback/sbsustart systemback</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="root-menu-49387" label="CONNECTIONS">
			<item label="Network Manager">
				<action name="Execute">
					<execute>nm-connection-editor</execute>
				</action>
			</item>
			<item label="Bluetooth Manager">
				<action name="Execute">
					<execute>blueman-manager</execute>
				</action>
			</item>
			<item label="Device Manager">
				<action name="Execute">
					<execute>gnome-terminal -e nmtui</execute>
				</action>
			</item>
		</menu>
		<separator/>
		<menu id="root-menu-735613" label="OPENBOX">
			<item label="OB Menu Edit">
				<action name="Execute">
					<execute>obmenu</execute>
				</action>
			</item>
			<item label="OB Preferences">
				<action name="Execute">
					<execute>obconf</execute>
				</action>
			</item>
			<item label="OB Reconfigure">
				<action name="Reconfigure"/>
			</item>
			<item label="OB Restart">
				<action name="Restart"/>
			</item>
		</menu>
		<separator/>
		<item label="Exit">
			<action name="Execute">
				<execute>/home/m/Stayboogy/cmds/openbox-logout</execute>
			</action>
		</item>
		<item label="Lock">
			<action name="Execute">
				<execute>dm-tool lock</execute>
			</action>
		</item>
		<item label="Reboot">
			<action name="Execute">
				<execute>sudo reboot</execute>
			</action>
		</item>
		<item label="Shutdown">
			<action name="Execute">
				<execute>sudo halt</execute>
			</action>
		</item>
	</menu>
</openbox_menu>
